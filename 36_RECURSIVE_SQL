/************* DATES ****************/
 WITH RECURSIVE DATE_TABLE AS (
  SELECT '2023-01-01'::DATE AS DA_DATE
  UNION ALL
  SELECT DA_DATE + 1 FROM DATE_TABLE
  WHERE DA_DATE < '2023-02-02'::DATE
)
SELECT * FROM DATE_TABLE;


/************* NUMBERS *******************/
 
 WITH RECURSIVE NUM_TABLE AS (
  SELECT 10 AS DA_NUM
  UNION ALL
  SELECT DA_NUM + 1 FROM NUM_TABLE
  WHERE DA_NUM < 100
)
SELECT * FROM NUM_TABLE;
